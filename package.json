{
	"name": "review-starter-syntax-highlight",
	"displayName": "Re:VIEW Starter Syntax Highlight",
	"description": "Syntax Highlights for Re:VIEW Starter.",
	"publisher": "yuqquu",
	"version": "1.0.3",
	"repository": {
		"type": "git",
		"url": "https://github.com/yuqquu/review-starter-syntax-highlight.git"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#f0f0f0",
		"theme": "light"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.25.1"
	},
	"main": "./extension.js",
	"activationEvents": [
		"*"
	],
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "review-starter",
				"aliases": [
					"Re:VIEW Starter",
					"review-starter"
				],
				"extensions": [
					".re"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "review-starter",
				"scopeName": "source.re",
				"path": "./syntaxes/review-starter.tmLanguage.json"
			}
		]
	}
}
